{
    "parser": "@typescript-eslint/parser",
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "airbnb-base",
      "stylelint-config-prettier",
      "prettier"
    ],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
}